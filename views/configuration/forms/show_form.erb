<div class="container">

	<form class="form-horizontal">	
		<fieldset>
			<center>
				<legend>Formulario</legend>
				<div class="form-group">
				  	<label class="col-md-2 control-label" for="form_name"></label>
				  	<div class="col-md-8">
					    <div class="input-group">
					    	<span class="input-group-addon">Nombre del Formulario</span>
					      	<input name="form_name" class="form-control" value ="<%= @form.form_name %>" type="text" readonly="">
					    </div>
				  	</div>
				</div>
			</center>
			<div class="body">
				<% @form.get_attributes_values.each do |attribute| %>
					<%= attribute.non_editable_type %>
				<%end%>
			</div>
		</fieldset>
	</form>
	<center>
		<div class="control-group">
		  	<div class="controls">
		    	<a id="go_back" class="btn btn-default">Cancelar</a>
		  	</div>
		</div>
	</center>
</div>
 

